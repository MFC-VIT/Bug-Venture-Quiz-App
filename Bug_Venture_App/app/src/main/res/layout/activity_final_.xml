<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/time_up_back"
    tools:context=".completed">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="36dp"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/mfc_logo" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_below="@id/imageView12"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/s_text" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                app:srcCompat="@drawable/c_u_sn"
                android:layout_below="@id/imageView13"
                android:layout_centerHorizontal="true"/>

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                app:srcCompat="@drawable/email_"
                android:layout_below="@id/imageView14"
                android:layout_marginLeft="40dp"/>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="241dp"
                android:layout_height="35dp"
                android:layout_marginTop="15dp"
                android:text="TextView"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:layout_below="@id/imageView15"
                android:layout_marginLeft="40dp" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="162dp"
                android:layout_height="31dp"
                android:layout_marginTop="15dp"
                android:layout_below="@id/textView4"
                app:srcCompat="@drawable/u_c"
                android:layout_marginLeft="40dp"/>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="224dp"
                android:layout_height="35dp"
                android:layout_marginTop="15dp"
                android:text="TextView"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:layout_below="@id/imageView16"
                android:layout_marginLeft="40dp"/>

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="147dp"
                android:layout_height="24dp"
                android:layout_marginTop="15dp"
                app:srcCompat="@drawable/total_time"
                android:layout_below="@id/textView5"
                android:layout_marginLeft="40dp"/>

            <TextView
                android:id="@+id/textView12"
                android:layout_width="220dp"
                android:layout_height="35dp"
                android:layout_marginTop="15dp"
                android:text="TextView"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:layout_below="@id/imageView20"
                android:layout_marginLeft="40dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="60dp"
                android:layout_below="@id/textView12">

            </LinearLayout>

        </RelativeLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
